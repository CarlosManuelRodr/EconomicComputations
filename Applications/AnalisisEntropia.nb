Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"GroupReturnsByRun", "[", "market_", "]"}], ":=", 
   RowBox[{"KeySort", "[", 
    RowBox[{"GroupBy", "[", 
     RowBox[{
      RowBox[{"Thread", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"TrendDuration", "[", 
          RowBox[{"market", "[", "\"\<Prices\>\"", "]"}], "]"}], ",", 
         RowBox[{"market", "[", "\"\<TrendReturns\>\"", "]"}]}], "}"}], "]"}],
       ",", 
      RowBox[{"First", "\[Rule]", "Last"}]}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ContinuousEntropy", "[", "data_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", "dist", "}"}], ",", "\[IndentingNewLine]", " ", 
     RowBox[{
      RowBox[{"dist", "=", " ", 
       RowBox[{"SmoothKernelDistribution", "[", "data", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"-", 
       RowBox[{"Quiet", "@", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"PDF", "[", 
            RowBox[{"dist", ",", "x"}], "]"}], "*", 
           RowBox[{"Log", "[", 
            FractionBox[
             RowBox[{"PDF", "[", 
              RowBox[{"dist", ",", "x"}], "]"}], 
             RowBox[{"PDF", "[", 
              RowBox[{
               RowBox[{"UniformDistribution", "[", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"Min", "[", "data", "]"}], ",", 
                  RowBox[{"Max", "[", "data", "]"}]}], "}"}], "]"}], ",", 
               "x"}], "]"}]], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{"Min", "[", "data", "]"}], ",", 
            RowBox[{"Max", "[", "data", "]"}]}], "}"}], ",", 
          RowBox[{"Method", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{"\"\<MonteCarlo\>\"", ",", 
             RowBox[{"\"\<MaxPoints\>\"", "\[Rule]", "3000000"}]}], "}"}]}], 
          ",", 
          RowBox[{"WorkingPrecision", "\[Rule]", "20"}], ",", 
          RowBox[{"MinRecursion", "\[Rule]", "4"}], ",", 
          RowBox[{"MaxRecursion", "\[Rule]", "200"}]}], "]"}]}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
